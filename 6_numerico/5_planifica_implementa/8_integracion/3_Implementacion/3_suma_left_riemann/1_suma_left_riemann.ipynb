    "def suma_izquierda(f, a, b, n):\n",
    "    import numpy as np \n",
    "    deltax = (b-a)/n # longitud de los subintervalos \n",
    "    particion = np.arange(a, b, deltax)\n",
    "    alturas = f(particion) \n",
    "    areas = alturas*deltax\n",
    "    return sum(areas)"
   ]
